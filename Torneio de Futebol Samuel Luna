<script>

// Define variáveis times e estados


function comb1() {
   
    var t1 = document.getElementById('time1').value;
    var e1 = document.getElementById('estado1').value;
		var t2 = document.getElementById('time2').value;
		var e2 = document.getElementById('estado2').value;
		var t3 = document.getElementById('time3').value;
		var e3 = document.getElementById('estado3').value;
		var t4 = document.getElementById('time4').value;
		var e4 = document.getElementById('estado4').value;
		var t5 = document.getElementById('time5').value;
		var e5 = document.getElementById('estado5').value;
		var t6 = document.getElementById('time6').value;
		var e6 = document.getElementById('estado6').value;
    
    var tt1 = document.getElementById('Rt1');
    tt1.innerHTML = t1;
    var tt2 = document.getElementById('Rt2');
    tt2.innerHTML = t2;
    var ee1 = document.getElementById('Re1');
    ee1.innerHTML = e1;
    
    

		var tt3 = document.getElementById('Rt3');
    tt3.innerHTML = t3;
    var tt4 = document.getElementById('Rt4');
    tt4.innerHTML = t4;
    var ee3 = document.getElementById('Re3');
    ee3.innerHTML = e3;

		var tt5 = document.getElementById('Rt5');
    tt5.innerHTML = t5;
    var tt6 = document.getElementById('Rt6');
    tt6.innerHTML = t6;
    var ee5 = document.getElementById('Re5');
    ee5.innerHTML = e5;


}


