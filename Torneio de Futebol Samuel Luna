// Recria as variáveis dos estados


 		var t1 = document.getElementById('time1').value;
    var t2 = document.getElementById('time2').value;
		var t3 = document.getElementById('time3').value;
		var t4 = document.getElementById('time4').value;
		var t5 = document.getElementById('time5').value;
		var t6 = document.getElementById('time6').value;
		   

// Cria as variáveis das pontuações totais


var pon1 = p11 + p12 + p13 + p14;
var pon2 = p21 + p22 + p23 + p24;
var pon3 = p31 + p32 + p33 + p34;
var pon4 = p41 + p42 + p43 + p44;
var pon5 = p51 + p52 + p53 + p54;
var pon6 = p61 + p62 + p63 + p64;

// Define variável para o maior valor de pontuação

var pcamp = Math.max(pon1, pon2, pon3, pon4, pon5, pon6);

 
 		  
document.getElementById('tab1').innerHTML = t1;  
document.getElementById('tab2').innerHTML = t2;  
document.getElementById('tab3').innerHTML = t3;  
document.getElementById('tab4').innerHTML = t4;  
document.getElementById('tab5').innerHTML = t5;  
document.getElementById('tab6').innerHTML = t6;  




document.getElementById('ptab1').innerHTML = pon1;  
document.getElementById('ptab2').innerHTML = pon2;  
document.getElementById('ptab3').innerHTML = pon3;  
document.getElementById('ptab4').innerHTML = pon4;  
document.getElementById('ptab5').innerHTML = pon5;  
document.getElementById('ptab6').innerHTML = pon6;  
document.getElementById('pcampeao').innerHTML = pcamp;  

 





}



</script>
