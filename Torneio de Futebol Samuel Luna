// Cria resultados aleatórios para os jogos


function resul() {
   
    
    var res11 = Math.floor(10* Math.random());    
    var vres11 = res11;
    document.getElementById('res11').innerHTML = res11;    
    
    var res21 = Math.floor(10* Math.random());    
    var vres21 = res21;
    document.getElementById('res21').innerHTML = res21;    
    
    
    var res31 = Math.floor(10* Math.random());    
    var vres31 = res31;
    document.getElementById('res31').innerHTML = res31;    

    
		var res41 = Math.floor(10* Math.random());    
    var vres41 = res41;
    document.getElementById('res41').innerHTML = res41;    
        
    var res51 = Math.floor(10* Math.random());    
    var vres51 = res51;
    document.getElementById('res51').innerHTML = res51;    
    
		var res61 = Math.floor(10* Math.random());    
    var vres61 = res61;
    document.getElementById('res61').innerHTML = res61;    
    
    
    
   	var res12 = Math.floor(10* Math.random());    
    var vres12 = res12;
    document.getElementById('res12').innerHTML = res12;
		
    var res22 = Math.floor(10* Math.random());    
    var vres22 = res22;
    document.getElementById('res22').innerHTML = res22;
		
    var res32 = Math.floor(10* Math.random());    
    var vres32 = res32;
    document.getElementById('res32').innerHTML = res32;
		
    var res42 = Math.floor(10* Math.random());    
    var vres42 = res42;
    document.getElementById('res42').innerHTML = res42;
		
    var res52 = Math.floor(10* Math.random());    
    var vres52 = res52;
    document.getElementById('res52').innerHTML = res52;
		
    var res62 = Math.floor(10* Math.random());    
    var vres62 = res62;
    document.getElementById('res62').innerHTML = res62;

    
    var res13 = Math.floor(10* Math.random());    
    var vres13 = res13;
    document.getElementById('res13').innerHTML = res13;

		var res23 = Math.floor(10* Math.random());    
    var vres23 = res23;
    document.getElementById('res23').innerHTML = res23;

    var res33 = Math.floor(10* Math.random());    
    var vres33 = res33;
    document.getElementById('res33').innerHTML = res33;

    var res43 = Math.floor(10* Math.random());    
    var vres43 = res43;
    document.getElementById('res43').innerHTML = res43;

		var res53 = Math.floor(10* Math.random());    
    var vres53 = res53;
    document.getElementById('res53').innerHTML = res53;
    
    var res63 = Math.floor(10* Math.random());    
    var vres63 = res63;
    document.getElementById('res63').innerHTML = res63;





    var res14 = Math.floor(10* Math.random());    
    var vres14 = res14;
    document.getElementById('res14').innerHTML = res14;

		var res24 = Math.floor(10* Math.random());    
    var vres24 = res24;
    document.getElementById('res24').innerHTML = res24;
    
    var res34 = Math.floor(10* Math.random());    
    var vres34 = res34;
    document.getElementById('res34').innerHTML = res34;
		
    var res44 = Math.floor(10* Math.random());    
    var vres44 = res44;
    document.getElementById('res44').innerHTML = res44;
		
    var res54 = Math.floor(10* Math.random());    
    var vres54 = res54;
    document.getElementById('res54').innerHTML = res54;
		
    var res64 = Math.floor(10* Math.random());    
    var vres64 = res64;
    document.getElementById('res64').innerHTML = res64;



// Adota as condições para pontuação : vitória = 3 pontos, empate = 1 ponto
   
   var part11 = vres11 - vres21;
        
if (part11 > 0) {
    	var p11 = 3;
      var p21 = 0;
    } 
    
    	else if (part11 < 0) {
    		var p11 = 0;
      	var p21 = 3;
    }
      
      else  {
     		var p11 = 1;
      	var p21 = 1;
    }  
 
 		  document.getElementById('pt11').innerHTML = p11;  
			document.getElementById('pt21').innerHTML = p21;  
      
      
      
      
      var part31 = vres31 - vres41;
        
if (part31 > 0) {
    	var p31 = 3;
      var p41 = 0;
    } 
    
    	else if (part31 < 0) {
    		var p31 = 0;
      	var p41 = 3;
    }
      
      else  {
     		var p31 = 1;
      	var p41 = 1;
    }  
 
 		  document.getElementById('pt31').innerHTML = p31;  
			document.getElementById('pt41').innerHTML = p41;  


